extends layout

block content
  span(style="width:40px;display:inline-block;") host
  input#memcacheHost(value="localhost:11211")
  input#memcacheLink(type="button",value="连接")
  span#message
  br
  span(style="width:40px;display:inline-block;") key
  input#key
  div
    input#query(type="button",value="查询")
    input#delNullKey(type="button",value="删除空键")
    table(style="border-collapse:collapse;border:none;border:solid #000 1px;")
      thead
        tr
          th success
          th key
          th operation
      tbody#dataList
  script(type="text/javascript", src="/javascripts/index.js")

