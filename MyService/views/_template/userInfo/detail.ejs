<div class="my-row">
    <div class="hidden">
        <input name="id" value="<%= id %>"/>
    </div>
    <div class="input-group">
        <label class="control-label"><%= account %>#<%= id %>(<%= nickname %>)</label>
    </div>
    <div class="input-group">
        <span class="input-group-addon">权限</span>
        <input type="text" class="form-control" name="authority"/>
    </div>
    <div class="input-group" name="authorityBox">
    </div>

    <div class="input-group">
        <span class="input-group-addon">角色</span>
        <input type="text" class="form-control" name="role"/>
    </div>
    <div class="input-group" name="roleBox">
    </div>

    <div class="input-group">
        <label class="control-label">架构</label><a name="refreshStruct" href="javascript:;">刷新</a>
    </div>
    <div name="structBox" class="row my-row">
        <div class="col-xs-2 struct"><select class="form-control"></select></div>
        <span class="text-danger msg"></span>
    </div>
</div>