<tbody class="itemRow">
<tr>
    <td><%= id %></td>
    <td><%= type_name %></td>
    <td><%= status_name %></td>
    <td>
        <span title="<%= account + '(' + nickname + '#' + user_info_id + ')' %>"><%= user_info %></span>
    </td>
    <td><%= title %></td>
    <td><%= description %></td>
    <td><%= create_date %></td>
    <td><%= operate_date %></td>
    <td><%= operator %></td>
    <td>
        <a href="javascript:;" class="itemDetailQuery">详细</a>
        <% if(common.isInArray('del', operation)){ %>
        <a href="javascript:;" class="itemDel statusUpdate" data-id="<%= id %>" data-operate="del">删除</a>
        <% } %>
    </td>
</tr>
</tbody>