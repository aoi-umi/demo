<div class="container">
    <% var rowItemClass = 'col-lg-3 col-md-3 col-sm-6 col-xs-12';%>
    <div class="form-group">
        <div class="row my-row">
            <%
            var prefix = '';
            var searchItemList = [{
                text: 'id',
                id:'id',
            },{
                text: 'type',
                id:'type',
            },{
                text: 'typeName',
                id:'typeName',
            },{
                text: 'parentType',
                id:'parentType',
            },{
                text: 'level',
                id:'level',
            },];
            for(var i = 0;i< searchItemList.length;i++){
                var item = searchItemList[i];
            %>
            <div class="<%=rowItemClass%>">
                <div class="input-group">
                    <span class="input-group-addon"><%=item.text%></span>
                    <input id="<%=prefix + item.id%>" type="text" class="form-control" />
                </div>
            </div>
            <%}%>
        </div>
        <div class="row my-row">
            <div class="<%=rowItemClass%> col-lg-offset-6 col-md-offset-6">
                <button id="search" class="btn btn-default btn-block">
                    search
                </button>
            </div>
            <div class="<%=rowItemClass%>">
                <button class="btn btn-default btn-block add">
                    add
                </button>
            </div>
        </div>
    </div>
    <table id="list" class="table table-bordered">
        <thead>
            <tr>
                <th class="text-center" colspan="100">mainContentType</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th>id</th><th>type</th><th>typeName</th>
                <th>parentType</th><th>level</th><th>操作</th>
            </tr>
        </tbody>
        <tbody class="error">
            <tr><td class="text-center" colspan="100" name="errorContent"></td></tr>
        </tbody>
    </table>
    <div id="pager"></div>

    <div>
        <a id="tree" href="javascript:;">tree</a>
        <div>
            <span>root</span>
            <ul class="tree" id="treeList">
            </ul>
            <span>not in root</span>
            <ul class="tree" id="notInRootTreeList">
            </ul>
        </div>
    </div>
</div>
<div id="detail" data-backdrop="false" role="dialog" tabindex="-1" class="modal fade pop-up-box">
    <% include ../../_template/detailModal%>
</div>
<script type="text/template" id="mainContentTypeSaveTemp">
    <% include ../../_template_front/mainContentType/detail%>
</script>
<script type="text/template" id="mainContentTypeItem">
    <% include ../../_template_front/mainContentType/listItem%>
</script>
<script type="text/template" id="mainContentTypeTreeItem">
    <% include ../../_template_front/mainContentType/treeItem%>
</script>
<script type="text/javascript">
    require(['./_main', './moduleMainContentType'], function () {
        var moduleMainContentType = require('./moduleMainContentType');
        require('./_main').init();        
        new moduleMainContentType.ModuleMainContentType();
    });
</script>