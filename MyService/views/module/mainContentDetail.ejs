<div class="container">
    <% var rowItemClass = 'col-xs-12 col-md-6';%>
    <div class="hidden">

    </div>
    <pre>
<%=JSON.stringify(mainContentDetail, null, '\t')%>
    </pre>
    <div class="form-group">
        <div>
            <p>
                <span><%=mainContentDetail.user_info_id || ''%></span>
            </p>
            <span>创建时间</span><span><%=mainContentDetail.create_date || ''%></span>
            <span>修改时间</span><span><%=mainContentDetail.operate_date || ''%></span>
            <span>修改人</span><span><%=mainContentDetail.operator || ''%></span>
        </div>
        <div class="row my-row">
            <div class="<%=rowItemClass%>">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">标题</label>
                        <div class="col-sm-10">
                            <input type="email" class="form-control" id="title" placeholder="" value="<%=mainContentDetail.title || ''%>"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">描述</label>
                        <div class="col-sm-10">
                            <textarea type="email" class="form-control" id="title" placeholder=""><%=mainContentDetail.desc || ''%></textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="/js/<%=env%>/my.pager.js?v=<%=version%>"></script>
<script type="text/javascript" src="/js/<%=env%>/module.js?v=<%=version%>"></script>
<script type="text/javascript" src="/js/<%=env%>/moduleMainContent.js?v=<%=version%>"></script>
<script type="text/javascript">
    $(function () {
        var opt = {
            operation:['detailQuery']
        };
        var mainContentDetail = moduleMainContent.init(opt);
    });
</script>