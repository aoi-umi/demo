<style>
    .itemConetnt .itemTitle {
        width: 200px;
    }
</style>
<div class="container">
    <div id="queryBox" class="form-group">
        <% var rowItemClass = 'col-lg-3 col-md-3 col-sm-6 col-xs-12'; %>
        <div class="row my-row">
            <%
            var searchItemList = [{
                text: 'id',
                name: 'id',
            }, {
                text: 'url',
                name: 'url',
            }, {
                text: 'application',
                name: 'application',
            }, {
                text: 'method',
                name: 'method',
            }, {
                text: 'methodName',
                name: 'methodName',
            }, {
                text: 'code',
                name: 'code',
            }, {
                text: 'createDateStart',
                name: 'createDateStart',
            }, {
                text: 'createDateEnd',
                name: 'createDateEnd',
            }, {
                text: 'guid',
                name: 'guid',
            }, {
                text: 'req',
                name: 'req',
            }, {
                text: 'res',
                name: 'res',
            }, {
                text: 'remark',
                name: 'remark',
            },];
            for(var i = 0;i < searchItemList.length;i++){
                var item = searchItemList[i];
            %>
            <div class="<%= rowItemClass %>">
                <div class="input-group">
                    <span class="input-group-addon"><%= item.text %></span>
                    <input name="<%= item.name %>" type="text" class="form-control"/>
                </div>
            </div>
            <% } %>
        </div>
        <div class="row my-row">
            <label class="control-label">结果</label>
            <label class="control-label">
                <input type="radio" name="result" value="" checked/>全部
            </label>
            <label class="control-label">
                <input type="radio" name="result" value="1"/>成功
            </label>
            <label class="control-label">
                <input type="radio" name="result" value="0"/>失败
            </label>
        </div>
        <div class="row my-row">
            <div class="<%= rowItemClass %> col-lg-offset-9 col-md-offset-9 col-sm-offset-6">
                <button id="query" class="btn btn-default btn-block">
                    search
                </button>
            </div>
        </div>
    </div>
    <table id="list" class="table table-bordered">
        <thead>
        <tr>
            <th class="text-center" colspan="100">log</th>
        </tr>
        </thead>
        <tbody class="error">
        <tr>
            <td class="text-center" colspan="100" name="errorContent"></td>
        </tr>
        </tbody>
    </table>
    <div id="pager"></div>
</div>
<script type="text/template" id="queryItemTemp">
    <% include ../_template_front/logItem %>
</script>
<script type="text/javascript">
	require(['./_main', './moduleLog'], function () {
		var moduleLog = require('./moduleLog');
		var main = require('./_main');
		main.init();
		new moduleLog.ModuleLog();
	})
</script>